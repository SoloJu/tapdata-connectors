      "to": "TapDate",
      "range": [
        "1000-01-01",
        "9999-12-31"
      ],
      "pattern": "yyyy-MM-dd"
    },
    "TIME [($fraction)] [WITHOUT TIME ZONE]": {
      "to": "TapTime",
      "byte": 8,
      "fraction": [
        0,
        6
